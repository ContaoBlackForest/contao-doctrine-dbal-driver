{
	"name":"contaoblackforest/contao-doctrine-dbal-driver",
	"description":"Database Driver using Doctrine DBAL for Contao Open Source CMS",
	"keywords":["contao", "database", "driver", "doctrine", "dbal"],
	"type":"contao-module",
	"license":"LGPL-3.0+",
	"authors":[
        {
          "name": "Sven Baumann",
          "email": "baumann.sv@gmail.com",
          "role": "Developer"
        },
        {
          "name": "Dominik Tomasi",
          "email": "dominik.tomasi@gmail.com",
          "role": "Developer"
        }
	],
    "support": {
      "issues": "https://github.com/ContaoBlackForest/contao-doctrine-dbal-driver/issues",
      "source": "https://github.com/ContaoBlackForest/contao-doctrine-dbal-driver"
    },
	"require":{
		"php":">=5.3",
		"contao/core": "3.1.*",
		"contao-community-alliance/composer-installer": "*",
		"doctrine/dbal": "2.4.*"
	},
	"autoload":{
		"psr-0":{
			"Database": "src/",
			"Contao\\Doctrine\\Driver": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-contao-2.11": "2.11.x-dev",
			"dev-contao-3.0": "3.0.x-dev",
			"dev-contao-3.1": "3.1.x-dev"
		}
	},
    "replace": {
      "bit3/contao-doctrine-dbal-driver": "self.version"
    }
}
